  "id": null,
  "uid": "observability-correlation",
  "title": "Observability Correlation - Logs, Traces & Metrics",
  "tags": [
    "observability",
    "correlation",
    "otel"
  ],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Service Overview",
      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${DS_TEMPO}"
          },
          "queryType": "",
          "refId": "A",
          "query": "{ status=ok }"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "list",
            "orientation": "horizontal"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Application Logs (with Trace Links)",
      "type": "logs",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${DS_LOKI}"
          },
          "expr": "{namespace_name=\"$namespace\", container_name=\"$service\", log_type=\"application\"} | json",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      },
      "gridPos": {
        "h": 12,
        "w": 12,
        "x": 0,
        "y": 6
      }
    },
    {
      "id": 3,
      "title": "Distributed Traces",
      "type": "traces",
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${DS_TEMPO}"
          },
          "query": "{resource.k8s.labels.app=\"$service\"}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 12,
        "w": 12,
        "x": 12,
        "y": 6
      }
    },
    {
      "id": 4,
      "title": "Service Map",
      "type": "nodeGraph",
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "${DS_TEMPO}"
          },
          "query": "{resource.k8s.labels.app=\"$service\"}",
          "refId": "A"
        }
      ],
      "options": {
        "nodes": {
          "mainStatUnit": "short"
        }
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 18
      }
    }
  ],
